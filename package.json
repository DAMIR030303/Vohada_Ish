{
  "name": "vohadaish",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "e2e": "detox test --configuration android.emu.debug",
    "e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "prepare": "husky",
    "test:full": "bash scripts/test-and-start.sh",
    "qa:check": "bash scripts/test-and-start.sh",
    "test:all": "bash scripts/run-tests.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.1",
    "axios": "^1.13.1",
    "babel-preset-expo": "^54.0.6",
    "expo": "~54.0.20",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.14",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.5.0",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "detox": "^20.45.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-renderer": "19.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
